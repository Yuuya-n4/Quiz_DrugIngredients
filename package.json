{
  "scripts": {
    "build:css": "sass ./app/assets/stylesheets/application.scss:./app/assets/builds/application.css ./app/assets/stylesheets/rails_admin.scss:./app/assets/builds/rails_admin.css ./app/assets/stylesheets/toppage.scss:./app/assets/builds/toppage.css --no-source-map --load-path=node_modules",
