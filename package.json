    "esbuild": "^0.19.10",
    "framer-motion": "^10.17.9",
    "prop-types": "^15.8.1",
    "rails_admin": "3.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-stars": "^2.2.5"
  },
  "packageManager": "yarn@1.22.19",
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^12.0.1",
    "sass": "^1.69.5",
    "tailwindcss": "^3.3.6"
  }
}
