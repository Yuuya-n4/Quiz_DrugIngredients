{
  "scripts": {
    "build:css": "sass ./app/assets/stylesheets/application.sass.scss:./app/assets/builds/application.css ./app/assets/stylesheets/rails_admin.scss:./app/assets/builds/rails_admin.css ./app/assets/stylesheets/toppage.scss:./app/assets/builds/toppage.css --no-source-map --load-path=node_modules",
    "build:tailwind": "npx tailwindcss -i ./app/assets/stylesheets/application.tailwind.css -o ./app/assets/builds/application.tailwind.css --minify"
  },
  "dependencies": {
    "rails_admin": "3.1.2"
  },
  "packageManager": "yarn@1.22.19",
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^12.0.1",
    "sass": "^1.69.5",
    "tailwindcss": "^3.3.6"
  }
}
